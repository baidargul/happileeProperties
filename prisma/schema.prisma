generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id      String    @id(map: "userss_pkey") @default(uuid())
  email   String?
  address String?
  phone   String?
  name    String
  builder builder[]
}

model builder {
  id          String  @id(map: "buyer_pkey") @default(uuid())
  userId      String
  description String?
  gst         String?
  users       users   @relation(fields: [userId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "buyer_userId_fkey")
}

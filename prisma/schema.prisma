generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model builder {
  id          String  @id @default(uuid())
  userId      String
  description String?
  gst         String?
  user        user    @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: Cascade, map: "builder_userId_fkey")
}

model user {
  id      String    @id @default(uuid())
  email   String?
  address String?
  phone   String?
  name    String
  builder builder[]
  image   image[]
}

model image {
  id     String @id(map: "images_pkey") @default(uuid())
  userId String
  url    String
  user   user   @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: Cascade, map: "images_userId_fkey")
}

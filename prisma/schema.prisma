generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model builder {
  id          String  @id @default(uuid())
  userId      String
  description String?
  gst         String?
  user        user    @relation(fields: [userId], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "builder_userId_fkey")
}

model user {
  id      String    @id @default(uuid())
  email   String?
  address String?
  phone   String?
  name    String
  builder builder[]
}
